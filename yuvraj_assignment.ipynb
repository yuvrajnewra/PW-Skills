# 34. Find the cumulative sum of a list
def cumulative_sum(lst):
    result = []
    total = 0
    for num in lst:
        total += num
        result.append(total)
    return result
# Example usage
lst = [1, 2, 3, 4]
cumsum = cumulative_sum(lst)
print("Cumulative Sum:", cumsum)

# 35. Find the average of all elements in a list
def find_average(lst):
