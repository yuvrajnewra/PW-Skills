# 27. Find all subsets of a list
def find_subsets(lst):
    subsets = [[]]
    for element in lst:
        subsets += [current + [element] for current in subsets]
    return subsets
# Example usage
lst = [1, 2, 3]
subsets = find_subsets(lst)
print("Subsets:", subsets)

